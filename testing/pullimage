node {
    stage('Pullimage') {
        sh 'docker pull fefefe8888/go-web-req-info:1.16-buster'
    }
    stage('twistlockScan') {
        prismaCloudScanImage ca: '', cert: '', dockerAddress: 'unix:///var/run/docker.sock', image: 'fefefe8888/go-web-req-info:16', key: '', logLevel: 'info', podmanPath: '', project: '', resultsFile: 'prisma-cloud-scan-results.json', ignoreImageBuildTime:true
    }
    stage('twistlockPublish') {
        prismaCloudPublish resultsFilePattern: 'prisma-cloud-scan-results.json'
    }
}
